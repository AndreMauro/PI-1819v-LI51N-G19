<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Listeners</th>
        </tr>
    </thead>
    <tbody>
        {{#each artist}}
            <tr>
                <td><a href="/#albums/{{name}}">{{name}}</td>
                <td>{{listeners}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>